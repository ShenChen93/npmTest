    "retry": "^0.10.1",
    "semver": "^5.3.0",
    "shelljs": "^0.8.2",
    "simple-git": "^3.15.0",
    "simple-oauth2": "1.0.3",
    "sugar": "^2.0.4",
    "tmp": "0.0.31",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.6.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "gulp": "^4.0.2",
    "mocha": "^6.2.0",
    "nyc": "^13.0.1",
    "proxyquire": "^1.7.11",
    "sinon": "^6.0.0"
  },
  "nyc": {
    "check-coverage": false,
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 90,
    "include": [
      "lib/*"
    ]
  },
  "_moduleAliases": {
    "@root": ".",
    "@src": "./lib",
    "@test": "./test"
  }
}
