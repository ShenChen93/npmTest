    "aws-sdk": "^2.288.0",
    "bluebird": "^3.5.1",
    "bunyan": "^1.8.12",
    "chalk": "2.4.2",
    "cli-color": "^1.0.0",
    "cli-spinner": "^0.2.6",
    "clui": "^0.3.1",
    "commander": "^2.9.0",
    "diff": "^3.5.0",
    "folder-hash": "^3.0.0",
    "fs-extra": "^2.1.0",
    "i18next": "^11.5.0",
    "inquirer": "^6.2.0",
    "is-valid-path": "^0.1.1",
    "js-yaml": "^3.13.1",
    "jsonfile": "^2.4.0",
    "mkdirp": "^0.5.1",
    "module-alias": "^2.1.0",
    "opn": "^4.0.2",
